#include <stdio.h>
#include "main.h"
/**
* print_priangle - function prints a triangle, followed by a new line
*
* @size: size parameter
* Return: 0
*/
void print_priangle(int size)
{
int a, int b;

if (size > 0)
{
for (a = 1; a <= size; a++)
{
for ((b = size - a); b > 0; b--)
{
putchar(' ');
}
for (b = 0; b < a; b++)
{
putchar('#');
}
if (a == size)
{
continue;
}
putchar('\n');
}
}
putchar('\n');
}
